<Window x:Class="eternalbox.desktop.liis17.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:eternalbox.desktop.liis17"
        xmlns:eo="http://schemas.essentialobjects.com/wpf/"
        mc:Ignorable="d"
        Title="Eternalbox Desktop (Beta)" Height="900" Width="950">
    <Grid>
        <eo:WebControl>
            <eo:WebControl.WebView>
                <eo:WebView ZoomFactor="0.75" x:Name="webmain" Url="eternalbox.dev">
                </eo:WebView>
            </eo:WebControl.WebView>
        </eo:WebControl>
    </Grid>
</Window>
